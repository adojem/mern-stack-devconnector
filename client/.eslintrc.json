{
   "env": {
      "browser": true
   },
   "rules": {
      "indent": ["error", 3, { "SwitchCase": 1}],
      "brace-style": [
         "error",
         "stroustrup"
      ],
      "no-shadow": "off",
      "react/jsx-indent": [
         "error",
         3
      ],
      "react/jsx-indent-props": [
         "error",
         3
      ],
      "react/jsx-filename-extension": "off"
   },
   "extends": [
      "airbnb"
   ]
}
